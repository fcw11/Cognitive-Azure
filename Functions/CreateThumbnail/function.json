{
  "bindings": [
    {
      "name": "input",
      "type": "blobTrigger",
      "direction": "in",
      "path": "images/{name}",
      "connection": "cognitiveazure_STORAGE"
    },
    {
      "type": "blob",
      "name": "output",
      "path": "thumbnails/{name}",
      "connection": "cognitiveazure_STORAGE",
      "direction": "out"
    },
    {
      "type": "table",
      "name": "inputTable",
      "tableName": "images",
      "rowKey": "{name}",
      "take": 1,
      "connection": "cognitiveazure_STORAGE",
      "direction": "in"
    }
  ],
  "disabled": false
}